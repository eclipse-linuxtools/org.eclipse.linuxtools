<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="org.eclipse.cdt.rpm.core.tests"
   name="RPM Tests Plug-in"
   version="3.0.0"
   provider-name="Eclipse.org"
   class="org.eclipse.linuxtools.rpm.core.tests.RPMCoreTestsPlugin">

   <runtime>
      <library name="tests.jar">
         <export name="*"/>
      </library>
   </runtime>

   <requires>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.linuxtools.rpm.core"/>
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="org.junit"/>
   </requires>

</plugin>
