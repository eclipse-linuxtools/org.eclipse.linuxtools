<ruleset comparisonMethod="maven"
	xmlns="https://www.mojohaus.org/VERSIONS/RULE/2.1.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="https://www.mojohaus.org/VERSIONS/RULE/2.1.0 https://www.mojohaus.org/versions/versions-model/xsd/rule-2.1.0.xsd">
	<!-- see
	https://www.mojohaus.org/versions/versions-maven-plugin/version-rules.html -->
	<ignoreVersions>
		<ignoreVersion type="regex">.+[-.](alpha|beta|b|M|RC|rc|pre.1)\d*$</ignoreVersion>
	</ignoreVersions>
	<rules>
		<rule artifactId="osgi-resource-locator" comparisonMethod="maven">
			<ignoreVersions>
				<ignoreVersion>3.0.0</ignoreVersion>
				<ignoreVersion>2.4.0</ignoreVersion>
			</ignoreVersions>
		</rule>
		<rule artifactId="jakarta.ws.rs-api" comparisonMethod="maven">
			<ignoreVersions>
				<ignoreVersion>4.0.0</ignoreVersion>
			</ignoreVersions>
		</rule>
		<rule groupId="org.glassfish.jersey.core" comparisonMethod="maven">
			<ignoreVersions>
				<ignoreVersion>4.0.2</ignoreVersion>
				<ignoreVersion>4.0.1</ignoreVersion>
			</ignoreVersions>
		</rule>
		<rule groupId="org.glassfish.jersey.ext" comparisonMethod="maven">
			<ignoreVersions>
				<ignoreVersion>4.0.2</ignoreVersion>
				<ignoreVersion>4.0.1</ignoreVersion>
			</ignoreVersions>
		</rule>
		<rule groupId="org.glassfish.jersey.media" comparisonMethod="maven">
			<ignoreVersions>
				<ignoreVersion>4.0.2</ignoreVersion>
				<ignoreVersion>4.0.1</ignoreVersion>
			</ignoreVersions>
		</rule>
		<rule groupId="org.glassfish.jersey.inject" comparisonMethod="maven">
			<ignoreVersions>
				<ignoreVersion>4.0.2</ignoreVersion>
				<ignoreVersion>4.0.1</ignoreVersion>
			</ignoreVersions>
		</rule>
	</rules>

</ruleset>